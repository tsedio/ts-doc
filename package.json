{
  "name": "@tsed/root",
  "version": "4.0.14",
  "description": "Generate documentation in markdown for TypeScript API",
  "main": "./src/index.js",
  "bin": "./bin/tsdoc.js",
  "private": true,
  "scripts": {
    "configure": "monorepo ci configure",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "eslint packages --ext .js",
    "test:lint:fix": "eslint packages --ext .js --fix",
    "build": "monorepo build",
    "sync:packages": "monorepo sync packages",
    "release": "semantic-release",
    "release:dryRun": "semantic-release --dry-run",
    "migrate": "ts-migrate-full ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsedio/ts-doc.git"
  },
  "keywords": [
    "Vuepress",
    "Typescript",
    "documentation",
    "API"
  ],
  "author": "Romakita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsedio/ts-doc/issues"
  },
  "homepage": "https://github.com/tsedio/ts-doc#readme",
  "dependencies": {
    "5to6-codemod": "^1.8.0",
    "lerna": "3.22.0",
    "ts-migrate": "^0.1.26"
  },
  "devDependencies": {
    "@tsed/monorepo-utils": "1.11.0",
    "@types/node": "^16.11.11",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/sinon-chai": "^3.2.7",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "prettier": "2.1.2",
    "proxyquire": "2.1.3",
    "semantic-release": "17.2.3",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "typescript": "^4.5.2",
    "ts-node": "^10.4.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "monorepo": {
    "productionBranch": "master",
    "developBranch": "master",
    "npmAccess": "public"
  }
}
